{
	"_version": "0.0.1",
	"sap.app": {
		"_version": "0.0.1",
		"id": "be.fiddle.sitbe",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "0.0.1"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"offline": true,
		"dataSources": {
			"info": {
				"uri": "./services/info.json",
				"type": "JSON"
			},
			"service.xsodata": {
				"uri": "./services/participants.php",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		},
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		}
	},
	"sap.ui": {
		"_version": "1.52.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_belize_plus"
		],
		"fullWidth": true
	},
	"sap.ui5": {
		"_version": "1.52.0",
		"rootView": {
			"viewName": "be.fiddle.sitbe.views.Main",
			"type": "XML",
			"id": "root"
		},
		"dependencies": {
			"minUI5Version": "1.52.0",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"info": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "info",
				"settings": {}
			},
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "be.fiddle.sitbe.i18n.i18n"
				}
			},
			"participants": {
				"__comment":"Every model must be named. non-named models are not acceptable",
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request",
					"useBatch" : false,
					"json":false
				},
				"dataSource": "service.xsodata",
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "./css/style.css"
				}
			]
		}
	}
}