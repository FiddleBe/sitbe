{
	"_version": "0.0.1",
	"sap.app":{
		"_version": "0.0.1",
		"id": "be.fiddle.sitbe",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "0.0.1"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"offline":true,
		"dataSources": {
			"info": {
				"uri": "./services/info.json",
				"type": "JSON"
			}
		}
	},
	"sap.ui": {
		"_version": "1.52.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [ "sap_belize_plus"],
		"fullWidth": true
	},
	"sap.ui5": {
		"_version": "1.52.0",
		"rootView": {
			"viewName": "be.fiddle.sitbe.Main",
			"type": "XML",
			"id":"root"
		},
		"dependencies": {
			"minUI5Version": "1.52.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"info": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {
                    "datasource":"info"
				}
			},
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "be.infra.sap.jsdoco.i18n.i18n"
				}
			}
		}
	}
}